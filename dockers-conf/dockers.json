{
    "storage": [
        {
            "name": "Nextcloud",
            "namespace": "storage",
            "description": "Personal cloud platform for file sync, sharing, and collaboration.",
            "docker_image": "nextcloud",
            "installed": false,
            "icon_url": "https://cdn.simpleicons.org/nextcloud",
            "install_script": "docker run -d -p 8003:80 --name Nextcloud nextcloud",
            "port": "8003"
        }
    ],
    "network": [
        {
            "name": "pi-hole",
            "namespace": "network",
            "description": "Network-wide ad and tracker blocker for local DNS resolution.",
            "docker_image": "pihole/pihole",
            "installed": false,
            "icon_url": "https://cdn.simpleicons.org/pi-hole",
            "install_script": "docker run -d -p 8006:80 --name pi-hole pihole/pihole",
            "port": "8006"
        }
    ],
    "security": [
        {
            "name": "Bitwarden",
            "namespace": "security",
            "description": "Self-hosted password manager compatible with Bitwarden.",
            "docker_image": "vaultwarden/server",
            "installed": false,
            "icon_url": "https://cdn.simpleicons.org/bitwarden",
            "install_script": "docker run -d -p 8007:80 --name Bitwarden vaultwarden/server",
            "port": "8007"
        }
    ],
    "media": [
        {
            "name": "Jellyfin",
            "namespace": "media",
